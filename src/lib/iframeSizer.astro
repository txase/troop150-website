---

interface Props {
  height: number;
}

const { height } = Astro.props;

---

<script is:inline define:vars={{ height }}>
  document.addEventListener('DOMContentLoaded', () => {
    var jotformFrame = document.querySelector("iframe");
    if (jotformFrame) {
      jotformFrame.style.minHeight = `${height}px`;
    }
  });
</script>
